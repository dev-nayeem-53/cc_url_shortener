{
  "writerOpts": {
    "commitsSort": false
  },
  "header": "# Changelog \n ### Notable Changes are added here.",
  "releaseCommitMessageFormat": "chore(release):   \uD83D\uDD16  releasing {{currentTag}}",
  "packageFiles": [
    {
      "filename": "./package.json",
      "type": "json"
    },
    {
      "filename": "./packages/backend/package.json",
      "type": "json"
    },
    {
      "filename": "./packages/web_app/package.json",
      "type": "json"
    }
  ],
  "bumpFiles": [
    {
      "filename": "./package.json",
      "type": "json"
    },
    {
      "filename": "./packages/backend/package.json",
      "type": "json"
    },
    {
      "filename": "./packages/web_app/package.json",
      "type": "json"
    }
  ],
  "types": [
    {
      "type": "feat",
      "section": "Features"
    },
    {
      "type": "fix",
      "section": "Bug Fixes"
    },
    {
      "type": "test",
      "section": "Tests"
    },
    {
      "type": "refactor",
      "section": "Code Refactored"
    },
    {
      "type": "style",
      "section": "Improved structure / format of the code"
    },
    {
      "type": "init",
      "section": "Initial Changes"
    }
  ]
